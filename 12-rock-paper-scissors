public Integer rockPaperScissors(String player1, String player2) {
    if (player1 == player2) {
        return 0; // tie
    }
    Map<String,String> jajanken = new Map<String,String> {
        'rock' : 'scissors',
        'scissors' : 'paper',
        'paper' : 'rock'
    };
    return (jajanken.get(player1) == player2) ? 1 : 2;
}