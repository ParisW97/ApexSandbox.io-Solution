public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<SObject> sobject_list = new List<SObject>();
    for(Account account : accounts) {
        sobject_list.add(Account(account.Name));
    }
    for(Contact contact : contacts) {
        sobject_list.add(Contact(contact.LastName));
    }
    return sobject_list;
}